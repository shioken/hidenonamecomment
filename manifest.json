{
    "name": "Hide noname comment",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Hide domesoccer blog noname comment Extension",
    "content_scripts": [
        {
            "matches": [
                "https://blog.domesoccer.jp/archives/*"
            ],
            "js": [
                "hide.js"
            ]
        }
    ]
}